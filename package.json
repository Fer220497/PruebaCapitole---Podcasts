    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "jpg"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
